@model IEnumerable<SMS.Models.sp_getTeacher_Result>
@{
    ViewBag.Title = "Index";
}



<h2 id="heads">List Of Classes</h2>
<div style="overflow: scroll;height: 500px; width: 70%;margin-left:15%;margin-right:15%;">
    <table id="MyTable" style="width:99%;  " class="table table-bordered">
        <thead >
            <tr>
                <th>Name</th>
                <th>Class</th>
                <th>Section</th>
                <th>Course</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ThFirstName &nbsp; @item.ThSecondName </td>
                    <td>@item.ClassName</td>
                    <td>@item.Name</td>
                    <td>@item.CoursesTitle</td>


                </tr>
            }
        </tbody>

    </table>

</div>