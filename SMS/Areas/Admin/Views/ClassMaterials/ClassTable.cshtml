@model IEnumerable<SMS.Models.ClassMaterial>


<table class="table ">
<tr>
        <th>ClassMaterials</th>
        <th>Actions</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="Search">
            <td>@item.Name</td>


            <td>
                <button class="btn btn-warning " id="btn-edit"  data-id="@item.ClassMaterialsId"><i class="glyphicon glyphicon-pencil"></i></button>
                <button class="btn btn-danger btn-delete" data-id="@item.ClassMaterialsId"><i class="glyphicon glyphicon-trash"></i></button>
            </td>
        </tr>
    }
    </table>
